{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "max-classes-per-file": "off",
      "no-use-before-define": "off",
      "no-plusplus": "off",
      "max-len":"warn",
      "no-continue":"off"
    }
}
